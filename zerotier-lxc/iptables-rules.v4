*nat
:PREROUTING ACCEPT [0:0]
:INPUT ACCEPT [0:0]
:OUTPUT ACCEPT [0:0]
:POSTROUTING ACCEPT [0:0]
-A POSTROUTING -o eth0 -s 192.168.191.0/24 -j SNAT --to-source 10.0.10.9
COMMIT

*filter
:INPUT ACCEPT [0:0]
:FORWARD DROP [0:0]
-A FORWARD -m conntrack --ctstate RELATED,ESTABLISHED -j ACCEPT
-A FORWARD -i zt+ -s 192.168.191.0/24 -d 10.0.10.0/24 -j ACCEPT
-A FORWARD -i eth0 -s 10.0.10.0/24 -d 192.168.191.0/24 -j ACCEPT
:OUTPUT ACCEPT [0:0]
COMMIT
